<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
      <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    </configSections>
    <!-- LOG INFO-->
    <log4net>
      <appender name="TaskCreatorLOG" type="log4net.Appender.FileAppender">
        <file value="TaskCreator.log" />
        <appendToFile value="true" />
        <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
        <layout type="log4net.Layout.PatternLayout">
          <conversionPattern value="%date %level %method %line %newline [Debug Message] - %message%newline" />
        </layout>
      </appender>
      <appender name="TaskDistributorLOG" type="log4net.Appender.FileAppender">
        <file value="TaskDistributor.log" />
        <appendToFile value="true" />
        <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
        <layout type="log4net.Layout.PatternLayout">
          <conversionPattern value="%date %level %method %line %newline [Debug Message] - %message%newline" />
        </layout>
      </appender>
      <appender name="IncentiveEngineLOG" type="log4net.Appender.FileAppender">
        <file value="IncentiveEngine.log" />
        <appendToFile value="true" />
        <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
        <layout type="log4net.Layout.PatternLayout">
          <conversionPattern value="%date %level %method %line %newline [Debug Message] - %message%newline" />
        </layout>
      </appender>
      <appender name="ReportAggregatorLOG" type="log4net.Appender.FileAppender">
        <file value="ReportAggregator.log" />
        <appendToFile value="true" />
        <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
        <layout type="log4net.Layout.PatternLayout">
          <conversionPattern value="%date %level %method %line %newline [Debug Message] - %message%newline" />
        </layout>
      </appender>
      <appender name="RepositoryLOG" type="log4net.Appender.FileAppender">
        <file value="Repository.log" />
        <appendToFile value="true" />
        <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
        <layout type="log4net.Layout.PatternLayout">
          <conversionPattern value="%date %level %method %line %newline [Debug Message] - %message%newline" />
        </layout>
      </appender>
      <appender name="TransactionNodeLOG" type="log4net.Appender.FileAppender">
        <file value="TransactionNode.log" />
        <appendToFile value="true" />
        <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
        <layout type="log4net.Layout.PatternLayout">
          <conversionPattern value="%date %level %method %line %newline [Debug Message] - %message%newline" />
        </layout>
      </appender>
      

      <root>
        <level value="ALL" />
      </root>
      
      <logger name="TaskCreatorLOG" additivity="false">
        <level value="ALL"/>
        <appender-ref ref="TaskCreatorLOG"/>
      </logger>
      <logger name="TaskDistributorLOG" additivity="false">
        <level value="ALL"/>
        <appender-ref ref="TaskDistributorLOG"/>
      </logger>
      <logger name="IncentiveEngineLOG" additivity="false">
        <level value="ALL"/>
        <appender-ref ref="IncentiveEngineLOG"/>
      </logger>
      <logger name="ReportAggregatorLOG" additivity="false">
        <level value="ALL"/>
        <appender-ref ref="ReportAggregatorLOG"/>
      </logger>
      <logger name="RepositoryLOG" additivity="false">
        <level value="ALL"/>
        <appender-ref ref="RepositoryLOG"/>
      </logger>
      <logger name="TransactionNodeLOG" additivity="false">
        <level value="ALL"/>
        <appender-ref ref="TransactionNodeLOG"/>
      </logger>

    </log4net>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
</configuration>